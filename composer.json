{
  "name": "gerfey/mapper",
  "version": "0.4",
  "type": "library",
  "description": "Map nested Json|Array structures.",
  "keywords": [
    "Gerfey",
    "Mapper",
    "Json",
    "Array",
    "JsonMapper",
    "ArrayMapper"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Levchenkov",
      "email": "sanjia@live.ru",
      "homepage": "https://github.com/gerfey",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-mbstring": "*",
    "ext-json": "*",
    "ext-reflection": "*",
    "doctrine/annotations": "^1.7"
  },
  "require-dev": {
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-reflection": "*"
  },
  "autoload": {
    "psr-4": {
      "Gerfey\\Mapper\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-4": {
      "Gerfey\\Mapper\\Tests\\": "tests/"
    }
  }
}